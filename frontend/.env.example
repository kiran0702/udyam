# Frontend Environment Variables Template
# Copy this file to .env and fill in your actual values

# ðŸš¨ NEVER commit .env files to version control
# All frontend env vars must be prefixed with VITE_ to be accessible

# API Configuration
VITE_API_URL="http://localhost:3000/registration"

# External API Keys (only non-sensitive, public keys)
VITE_PINCODE_API_KEY="your-public-pincode-api-key"
VITE_GOOGLE_MAPS_API_KEY="your-google-maps-api-key"
VITE_ANALYTICS_KEY="your-google-analytics-key"

# Application Configuration
VITE_APP_NAME="Udyam Registration Portal"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="Government of India - MSME Registration Portal"

# Environment
VITE_NODE_ENV="development"

# Debug Settings (for development only)
VITE_DEBUG_MODE="true"
VITE_ENABLE_CONSOLE_LOGS="true"

# Feature Flags
VITE_ENABLE_PIN_AUTOFILL="true"
VITE_ENABLE_PROGRESS_TRACKER="true"
VITE_ENABLE_VALIDATION="true"
VITE_ENABLE_GOVERNMENT_THEME="true"

# UI Configuration
VITE_THEME_COLOR="#4F46E5"
VITE_BRAND_COLOR="#7C3AED"

# Performance Configuration
VITE_ENABLE_PWA="false"
VITE_ENABLE_SERVICE_WORKER="false"

# SEO Configuration
VITE_SITE_URL="https://udyam-registration.onrender.com"
VITE_SITE_TITLE="Udyam Registration - MSME Portal"
VITE_SITE_DESCRIPTION="Official Udyam Registration Portal for Micro, Small and Medium Enterprises"
